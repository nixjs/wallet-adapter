{
    "name": "@nixjs23n6/transaction-adapter",
    "version": "1.1.44",
    "description": "Libraries that help you get transaction information on the blockchain.",
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "browser": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "keywords": [
        "transaction",
        "typescript",
        "web"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nixjs/wallet-adapter.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "author": {
        "url": "https://github.com/nixjs",
        "name": "nghinv"
    },
    "license": "MIT",
    "files": [
        "lib"
    ],
    "scripts": {
        "clean": "rimraf lib/*",
        "build:pre": "run-s clean",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build": "run-s build:pre build:cjs build:esm",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
    },
    "dependencies": {
        "@mysten/bcs": "^0.5.0",
        "@mysten/sui.js": "^0.20.0",
        "aptos": "^1.4.0",
        "async-sema": "^3.1.1"
    },
    "peerDependencies": {
        "@nixjs23n6/asset-adapter": "^1.1.44",
        "@nixjs23n6/hd-wallet-adapter": "^1.1.44",
        "@nixjs23n6/objectify": ">=0.0.1",
        "@nixjs23n6/types": "^1.0.15",
        "@nixjs23n6/utilities-adapter": "^1.1.44"
    },
    "devDependencies": {
        "@nixjs23n6/asset-adapter": "^1.1.44",
        "@nixjs23n6/hd-wallet-adapter": "^1.1.44",
        "@nixjs23n6/objectify": ">=0.0.1",
        "@nixjs23n6/types": "^1.0.15",
        "@nixjs23n6/utilities-adapter": "^1.1.44",
        "@types/lodash-es": "^4.17.6",
        "lodash-es": "^4.17.21"
    }
}
