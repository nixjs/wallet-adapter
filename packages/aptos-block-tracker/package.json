{
    "name": "@nixjs23n6/aptos-block-tracker",
    "version": "1.2.0",
    "description": "A JS module for keeping track of the latest Aptos block",
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "browser": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "keywords": [
        "tracker",
        "aptos",
        "block-tracker",
        "web"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nixjs/wallet-adapter.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "author": {
        "url": "https://github.com/nixjs",
        "name": "nghinv"
    },
    "license": "MIT",
    "files": [
        "lib"
    ],
    "scripts": {
        "clean": "rimraf lib/*",
        "build:pre": "run-s clean",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build": "run-s build:pre build:cjs build:esm",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
    },
    "dependencies": {
        "aptos": "^1.4.0",
        "axios": ">1.1.0",
        "@metamask/safe-event-emitter": "^2.0.0"
    },
    "devDependencies": {
        "typescript": "^4.9.4"
    }
}
