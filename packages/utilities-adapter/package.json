{
  "name": "@nixjs23n6/utilities-adapter",
  "version": "1.1.9",
  "description": "The utilities",
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "keywords": [
    "transaction",
    "typescript",
    "web"
  ],
  "repository": {
